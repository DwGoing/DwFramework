<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <ReleaseVersion>2.1.1</ReleaseVersion>
        <SubVersion>4</SubVersion>
        <Version>$(ReleaseVersion).$(SubVersion)</Version>
        <Authors>DwGoing</Authors>
        <Description>DwFramework 机器学习库</Description>
        <PackageProjectUrl>https://github.com/Dwgoing/DwFramework</PackageProjectUrl>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\DwFramework.Core\DwFramework.Core.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.ML" Version="1.5.2" />
        <PackageReference Include="Microsoft.ML.Recommender" Version="0.17.2" />
        <PackageReference Include="Microsoft.ML.TimeSeries" Version="1.5.2" />
    </ItemGroup>
    <ItemGroup>
        <None Include="README.md">
            <IncludeInPackage>true</IncludeInPackage>
        </None>
        <None Include="..\..\..\LICENSE" Link="LICENSE">
            <IncludeInPackage>true</IncludeInPackage>
        </None>
    </ItemGroup>
</Project>