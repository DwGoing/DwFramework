<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <ReleaseVersion>2.0.2</ReleaseVersion>
        <SubVersion>6</SubVersion>
        <Version>$(ReleaseVersion).$(SubVersion)</Version>
        <Authors>DwGoing</Authors>
        <Description>DwFramework RPCåº“</Description>
        <PackageProjectUrl>https://github.com/Dwgoing/DwFramework</PackageProjectUrl>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\DwFramework.Core\DwFramework.Core.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Google.Protobuf" Version="3.13.0" />
        <PackageReference Include="Grpc.Core" Version="2.33.1" />
        <PackageReference Include="Grpc.Tools" Version="2.33.1">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
    </ItemGroup>
    <ItemGroup>
        <None Include="README.md">
            <IncludeInPackage>true</IncludeInPackage>
        </None>
        <None Include="..\..\..\LICENSE" Link="LICENSE">
            <IncludeInPackage>true</IncludeInPackage>
        </None>
    </ItemGroup>
</Project>