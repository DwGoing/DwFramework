<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <ReleaseVersion>2</ReleaseVersion>
        <BuildVersion>2.56.8-rc.8174</BuildVersion>
        <Version>$(ReleaseVersion).$(BuildVersion)</Version>
        <Authors>DwGoing</Authors>
        <Description>DwFramework WebAPI插件库</Description>
        <PackageProjectUrl>https://github.com/Dwgoing/DwFramework</PackageProjectUrl>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\..\Module\DwFramework.WebAPI\DwFramework.WebAPI.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="6.8.0" />
        <PackageReference Include="Consul" Version="1.6.1.1" />
        <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.8.0" />
        <PackageReference Include="Swashbuckle.AspNetCore" Version="5.6.3" />
        <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="3.1.10" />
    </ItemGroup>
    <ItemGroup>
        <None Include="README.md">
            <IncludeInPackage>true</IncludeInPackage>
        </None>
        <None Include="..\..\..\LICENSE" Link="LICENSE">
            <IncludeInPackage>true</IncludeInPackage>
        </None>
    </ItemGroup>
</Project>